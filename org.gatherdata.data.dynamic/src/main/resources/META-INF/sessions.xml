<?xml version="1.0" encoding="UTF-8"?>
<sessions version="1.0.2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <session xsi:type="database-session">
      <name>dynamo</name>
      <event-listener-classes/>
      <logging xsi:type="eclipselink-log"/>
      <login xsi:type="database-login">
         <platform-class>org.eclipse.persistence.platform.database.HSQLPlatform</platform-class>
         <user-name>sa</user-name>
         <password></password>
         <sequencing>
            <default-sequence xsi:type="table-sequence">
               <name>Default</name>
            </default-sequence>
         </sequencing>
         <driver-class>org.hsqldb.jdbcDriver</driver-class>
         <connection-url>jdbc:hsqldb:hsql://localhost/gather</connection-url>
         <struct-converters/>
      </login>
   </session>
</sessions>
